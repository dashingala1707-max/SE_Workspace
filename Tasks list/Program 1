#include <stdio.h>

int main()
{
    int a = 37;
    int b = 37;
    int c = 39;
    int d = 37;
    int e = 36;

    int ans = a + b + c + d + e;
    float ans1 = (a + b + c + d + e) * 100.0 / (40 * 5);

    printf("Your total marks is : %d\n", ans);
    printf("Your percentage is : %f \n", ans1);

    return 0;
}
